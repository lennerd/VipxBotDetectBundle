{
    "name": "vipx/bot-detect-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VipxBotDetectBundle",
    "keywords": ["Security", "Bot"],
    "homepage": "http://lennerd.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Hildebrandt",
            "email": "hello@lennerd.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/security": "^3.0",
        "symfony/security-bundle": "^3.0",
        "vipx/bot-detect": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.5",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": { "Vipx\\BotDetectBundle\\": "./" }
    },
    "autoload-dev": {
        "psr-4": { "Vipx\\BotDetectBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
